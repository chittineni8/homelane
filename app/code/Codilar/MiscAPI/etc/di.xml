<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Codilar\MiscAPI\Api\GetErpSkuManagementInterface"
                type="Codilar\MiscAPI\Model\GetErpSkuManagement"/>
    <preference for="Codilar\MiscAPI\Api\GetBomTypeManagementInterface"
                type="Codilar\MiscAPI\Model\GetBomTypeManagement"/>
    <preference for="Codilar\MiscAPI\Api\GetSkuBySearchManagementInterface"
                type="Codilar\MiscAPI\Model\GetSkuBySearchManagement"/>
    <preference for="Codilar\MiscAPI\Api\GetPriceBySkuManagementInterface"
                type="Codilar\MiscAPI\Model\GetPriceBySkuManagement"/>
    <type name="Codilar\MiscAPI\Logger\Handler">
        <arguments>
            <argument name="filesystem" xsi:type="object">Magento\Framework\Filesystem\Driver\File</argument>
        </arguments>
    </type>
    <type name="Codilar\MiscAPI\Logger\Logger">
        <arguments>
            <argument name="name" xsi:type="string">Codilar_Erp_Api</argument>
            <argument name="handlers" xsi:type="array">
                <item name="system" xsi:type="object">Codilar\MiscAPI\Logger\Handler</item>
            </argument>
        </arguments>
    </type>
</config>
