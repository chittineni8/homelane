<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/erp-sku" method="POST">
		<service class="Codilar\MiscAPI\Api\GetErpSkuManagementInterface" method="getErpSku"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route url="/V1/bom-type" method="POST">
		<service class="Codilar\MiscAPI\Api\GetBomTypeManagementInterface" method="getBomType"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route url="/V1/sku-search" method="POST">
		<service class="Codilar\MiscAPI\Api\GetSkuBySearchManagementInterface" method="getSkuBySearch"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>
