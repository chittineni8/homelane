<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">codilar_vendor_vendor_form.vendor_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Codilar\Vendor\Block\Adminhtml\Vendor\Edit\BackButton"/>
			<button name="delete" class="Codilar\Vendor\Block\Adminhtml\Vendor\Edit\DeleteButton"/>
			<button name="save" class="Codilar\Vendor\Block\Adminhtml\Vendor\Edit\SaveButton"/>
			<button name="save_and_continue" class="Codilar\Vendor\Block\Adminhtml\Vendor\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>codilar_vendor_vendor_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>codilar_vendor_vendor_form.vendor_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="vendor_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="vendor_form_data_source" class="Codilar\Vendor\Model\Vendor\DataProvider">
			<settings>
				<requestFieldName>vendor_id</requestFieldName>
				<primaryFieldName>vendor_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="automat_vendor_id" formElement="textarea" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Vendor</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">automat_vendor_id</label>
				<dataScope>automat_vendor_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="automat_vendor_name" formElement="textarea" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Vendor</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">automat_vendor_name</label>
				<dataScope>automat_vendor_name</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="sap_vendor_id" formElement="textarea" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Vendor</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">sap_vendor_id</label>
				<dataScope>sap_vendor_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="sap_vendor_name" formElement="textarea" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Vendor</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">sap_vendor_name</label>
				<dataScope>sap_vendor_name</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="city" formElement="textarea" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Vendor</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">city</label>
				<dataScope>city</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="lead_time" formElement="textarea" sortOrder="60">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Vendor</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">lead_time</label>
				<dataScope>lead_time</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="buying_price" formElement="textarea" sortOrder="70">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Vendor</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">buying_price</label>
				<dataScope>buying_price</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="sku" formElement="textarea" sortOrder="80">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Vendor</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">sku</label>
				<dataScope>sku</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="vendor_sku" formElement="textarea" sortOrder="90">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Vendor</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">vendor_sku</label>
				<dataScope>vendor_sku</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="vendor_sku_status" formElement="textarea" sortOrder="100">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Vendor</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">vendor_sku_status</label>
				<dataScope>vendor_sku_status</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
