<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">codilar_priceversion_priceversiondetails_listing.codilar_priceversion_priceversiondetails_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>codilar_priceversion_priceversiondetails_columns</spinner>
		<deps>
			<dep>codilar_priceversion_priceversiondetails_listing.codilar_priceversion_priceversiondetails_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add new Priceversiondetails</label>
			</button>
		</buttons>
	</settings>
	<dataSource name="codilar_priceversion_priceversiondetails_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">priceversiondetails_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Codilar_Priceversion::Priceversiondetails</aclResource>
		<dataProvider name="codilar_priceversion_priceversiondetails_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>priceversiondetails_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
		<exportButton class="Magento\Ui\Component\ExportButton">
					 <argument name="data" xsi:type="array">
							 <item name="config" xsi:type="array">
									 <item name="options" xsi:type="array">
											 <item name="csv" xsi:type="array">
													 <item name="value" xsi:type="string">csv</item>
													 <item name="label" xsi:type="string" translate="true">CSV</item>
													 <item name="url" xsi:type="string">mui/export/gridToCsv</item>
											 </item>
											 <item name="xml" xsi:type="array">
													 <item name="value" xsi:type="string">xml</item>
													 <item name="label" xsi:type="string" translate="true">Excel XML</item>
													 <item name="url" xsi:type="string">mui/export/gridToXml</item>
											 </item>
									 </item>
							 </item>
					 </argument>
			 </exportButton>
	</listingToolbar>
	<columns name="codilar_priceversion_priceversiondetails_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">codilar_priceversion_priceversiondetails_listing.codilar_priceversion_priceversiondetails_listing.codilar_priceversion_priceversiondetails_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">priceversiondetails_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" xsi:type="url" path="codilar_priceversion/Priceversiondetails/inlineEdit"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">codilar_priceversion_priceversiondetails_listing.codilar_priceversion_priceversiondetails_listing.codilar_priceversion_priceversiondetails_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>priceversiondetails_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="priceversiondetails_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="price_version_id" component="Magento_Ui/js/grid/columns/select" >
			<settings>
				<filter>text</filter>
				<label translate="true">Price Version</label>
				<options class="Codilar\Priceversion\Model\Priceversiondetails\Options"/>
				<filter>select</filter>
				<dataType>select</dataType>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>

		<column name="sku">
			<settings>
				<filter>text</filter>
				<label translate="true">SKU</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="price">
			<settings>
				<filter>text</filter>
				<label translate="true">Price</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="add_price">
			<settings>
				<filter>text</filter>
				<label translate="true">Additional Price</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="multi">
			<settings>
				<filter>text</filter>
				<label translate="true">Multiplier</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="min_order_qty">
			<settings>
				<filter>text</filter>
				<label translate="true">Min Order Qty</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="min_order_value">
			<settings>
				<filter>text</filter>
				<label translate="true">Min Order Value</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="status" component="Magento_Ui/js/grid/columns/select" >
			<settings>
				<filter>text</filter>
				<label translate="true">Status</label>
				<options class="Magento\Config\Model\Config\Source\Enabledisable"/>
				<filter>select</filter>
				<dataType>select</dataType>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="created_at">
			<settings>
				<filter>text</filter>
				<label translate="true">Created At</label>
			</settings>
		</column>
		<column name="updated_at">
			<settings>
				<filter>text</filter>
				<label translate="true">Updated At</label>
			</settings>
		</column>
		<actionsColumn name="actions" class="Codilar\Priceversion\Ui\Component\Listing\Column\PriceversiondetailsActions">
			<settings>
				<indexField>priceversiondetails_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
	</columns>
</listing>
