<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">codilar_priceversion_priceversiondetails_form.priceversiondetails_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Codilar\Priceversion\Block\Adminhtml\Priceversiondetails\Edit\BackButton"/>
			<button name="delete" class="Codilar\Priceversion\Block\Adminhtml\Priceversiondetails\Edit\DeleteButton"/>
			<button name="save" class="Codilar\Priceversion\Block\Adminhtml\Priceversiondetails\Edit\SaveButton"/>
			<button name="save_and_continue" class="Codilar\Priceversion\Block\Adminhtml\Priceversiondetails\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>codilar_priceversion_priceversiondetails_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>codilar_priceversion_priceversiondetails_form.priceversiondetails_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="priceversiondetails_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="priceversiondetails_form_data_source" class="Codilar\Priceversion\Model\Priceversiondetails\DataProvider">
			<settings>
				<requestFieldName>priceversiondetails_id</requestFieldName>
				<primaryFieldName>priceversiondetails_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="price_version_id" formElement="input" sortOrder="90">
			<argument name="data" xsi:type="array">
				<item name="options" xsi:type="object">Codilar\Priceversion\Model\Priceversiondetails\Options</item>
				<item name="config" xsi:type="array">
						<item name="dataType" xsi:type="string">text</item>
						<item name="label" xsi:type="string" translate="true">Price Version</item>
						<item name="formElement" xsi:type="string">select</item>
						<item name="source" xsi:type="string">Priceversiondetails</item>
						<item name="dataScope" xsi:type="string">price_version_id</item>
						<item name="caption" xsi:type="string" translate="true">-- Please Select --</item>
				</item>
		</argument>
		</field>
		<field name="sku" formElement="textarea" sortOrder="100">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Priceversiondetails</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">SKU</label>
				<dataScope>sku</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="price" formElement="textarea" sortOrder="110">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Priceversiondetails</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Price</label>
				<dataScope>price</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="add_price" formElement="textarea" sortOrder="120">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Priceversiondetails</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Additional Price</label>
				<dataScope>add_price</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="multi" formElement="textarea" sortOrder="130">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Priceversiondetails</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Multiplier</label>
				<dataScope>multi</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="min_order_qty" formElement="textarea" sortOrder="140">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Priceversiondetails</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Min Order Qty</label>
				<dataScope>min_order_qty</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="min_order_value" formElement="textarea" sortOrder="150">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Priceversiondetails</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Min Order Value</label>
				<dataScope>min_order_value</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="status" formElement="select" sortOrder="160">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Priceversiondetails</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Status</label>
				<dataScope>status</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
			<formElements>
        <select>
            <settings>
                <options class="Magento\Config\Model\Config\Source\Enabledisable"/>
            </settings>
        </select>
    </formElements>
		</field>
	</fieldset>
</form>
