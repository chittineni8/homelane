<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <css src="Webkul_MpZipCodeValidator::css/styles.css"/>
    </head>
    <body>
        <referenceContainer name="marketplace_product_edit">
            <block class="Webkul\MpZipCodeValidator\Block\Product\EditRegion" name="mpzipcodevalidator_editqty" template="product/edit.phtml" cacheable="false" ifconfig="mpzipcode/general/enable">
                <arguments>
                    <argument xsi:type="helper" helper="\Webkul\MpZipCodeValidator\Helper\Data::getZipCodeValidationOptions" name="zipcodevalidationOptionsonEdit"></argument>
                </arguments>
            </block>
        </referenceContainer> 
    </body>
</page>

