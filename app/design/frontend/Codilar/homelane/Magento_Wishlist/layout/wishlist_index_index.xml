<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="customer.wishlist.item.cart" remove="true" />
        <referenceBlock name="customer.wishlist.item.comment" remove="true" />
        <referenceBlock name="customer.wishlist.buttons" remove="true" />
        <referenceBlock name="customer.wishlist.items">
            <arguments>
                <argument name="wishlistpriceVieModel" xsi:type="object">Codilar\CategoryFilter\ViewModel\Category</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
